colorscheme murphy
if has("win32")
  set guifont=Consolas,Fixedsys 
else
  set guifont=Ubuntu\ Mono\ 10,Consolas\ 12,Inconsolata\ 13,DejaVu\ Sans\ Mono\ 10
endif
set backspace=indent,eol,start
set guioptions-=m
set guioptions-=T
set guioptions-=r
set guioptions-=L
set guioptions-=b

highlight ColorColumn guibg=#000000 guifg=#880000
